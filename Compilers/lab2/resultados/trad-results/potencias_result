(setq potencias (make-array 100))
(setq m 0)
(defun main()
    (setq main_i 0)
    (setf m 30)
    (setf main_i 1)
    (setf (aref potencias 0) 1)
    (loop while (<= main_i m) do
        (setf (aref potencias main_i) (* (aref potencias (- main_i 1)) 2))
        (setf main_i (+ main_i 1))
    )
    (setf main_i 1)
    (loop while (<= main_i m) do
        (princ main_i) (princ " ") (princ (aref potencias main_i)) 
        (print "")
        (setf main_i (+ main_i 1))
    )
)

 (main)
