(defun factoriala (factoriala_n factoriala_f factoriala_fm)
    (if (= factoriala_n 1)
        (progn (princ factoriala_f) (princ " ") 
        )
        (progn (factoriala (- factoriala_n 1) (* factoriala_f factoriala_fm) (- factoriala_fm 1))
    ))
)

(defun factorialwrapper (factorialwrapper_n)
    (factoriala factorialwrapper_n factorialwrapper_n (- factorialwrapper_n 1))
    (print "")
)

(defun main()
    (factorialwrapper 1)
    (factorialwrapper 2)
    (factorialwrapper 3)
    (factorialwrapper 4)
    (factorialwrapper 5)
    (factorialwrapper 6)
)

 (main)
