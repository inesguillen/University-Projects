(defun cuadrado (cuadrado_a)
    (setq cuadrado_c 0)
    (setf cuadrado_c (* cuadrado_a cuadrado_a))
    (return-from cuadrado cuadrado_c)
)

(defun main()
    (setq main_a 0)
    (setq main_c 0)
    (setf main_a 7)
    (print "El cuadrado de ")
    (princ main_a) 
    (setf main_c (cuadrado  main_a))
    (print " es ")
    (princ main_c) 
    (setf main_a 12)
    (princ "El cuadrado de ") (princ main_a) (princ " es: ") (princ (cuadrado  main_a)) 
    (print " ")
)

 (main)
